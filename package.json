{
  "name": "vaylaithap-articles",
  "version": "0.1.0",
  "description": "vaylaithap articles domain service",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh@gitlab.vred.vn:backend/services/vaylaithap-articles.git"
  },
  "keywords": [
    "vaylaithap",
    "article",
    "service"
  ],
  "author": "manh.tran@vred.vn",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://gitlab.vred.vn/backend/services/vaylaithap-articles/issues"
  },
  "homepage": "https://gitlab.vred.vn/backend/services/vaylaithap-articles#readme",
  "engines": {
    "node": "10.15.0"
  },
  "main": "./src/index.ts",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prestart": "npm run lint",
    "start": "nodemon ./src/index.ts"
  },
  "nodemonConfig": {
    "verbose": false,
    "watch": [
      "src"
    ],
    "legacyWatch": true,
    "execMap": {
      "ts": "ts-node"
    },
    "env": {
      "NODE_ENV": "development",
      "DEBUG": "app:*",
      "DEBUG_HIDE_DATE": false,
      "DEBUG_COLORS": true,
      "DEBUG_DEPTH": null,
      "DEBUG_SHOW_HIDDEN": true
    },
    "ext": "ts,json"
  },
  "dependencies": {
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "morgan": "1.9.1",
    "pg": "7.8.0",
    "pg-hstore": "2.3.2",
    "sequelize": "4.42.0",
    "prisma-client-lib": "1.24.0",
    "ts-node": "7.0.1",
    "typescript": "3.2.2",
    "@types/dotenv": "6.1.0",
    "@types/express": "4.16.0",
    "@types/graphql": "14.0.4",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.18",
    "@types/ws": "6.0.1"
  },
  "devDependencies": {
    "@types/jest": "23.3.12",
    "jest": "23.6.0",
    "nodemon": "1.18.9",
    "supertest": "3.3.0",
    "ts-jest": "23.10.5",
    "tslint": "5.12.1",
    "tslint-config-standard": "8.0.1"
  }
}
